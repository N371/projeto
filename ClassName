public classClassName{
public ClassName( String nome, Int idade, String sexo,)
{
this.nome = nome;
this.idade = idade;
this.sexo = sexo;
}
public ClassName(){
}
public void setnome(String nome) {
      this.nome=nome;
}
public void setidade(Int idade) {
      this.idade=idade;
}
public void setsexo(String sexo) {
      this.sexo=sexo;
}
public String getnome() {
      return this.nome;
}
public Int getidade() {
      return this.idade;
}
public String getsexo() {
      return this.sexo;
}
@Override
 public String toString(){
 return ClassName{"nome=" + nome "idade=" + idade "sexo=" + sexo }
}
